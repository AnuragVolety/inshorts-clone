{
  "expo": {
    "name": "inshorts-clone",
    "slug": "inshorts-clone",
    "platforms": [
      "ios",
      "android"
      ],
    "sdkVersion": "37.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
