      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anurag.inshortsclone"
    },
    "android": {
      "package": "com.anurag.inshortsclone"
    }
  }
}
